{
  "name": "trawlr",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "all": "pm2 start ecosystem.config.js",
    "googlesearch:install": "cd GoogleSearchCrawler && pip install -r requirements.txt",
    "postinstall": "mkdir -p data&&touch data/db.json&&cp -n .env.template .env",
    "prettier:check": "prettier --check \"{src,services,datasources}/**/*.{vue,js,ts}\"",
    "prettier:write": "prettier --write \"{src,services,datasources}/**/*.{vue,js,ts}\"",
    "test": "jest",
    "semantic-release": "semantic-release",
    "update-dependencies": "ncu -u",
    "generate-readme": "cd scripts && ./build-docu.sh"
  },
  "release": {
    "analyzeCommits": "@semantic-release/commit-analyzer",
    "verifyConditions": "@semantic-release/npm",
    "prepare": "@semantic-release/npm",
    "generateNotes": "@semantic-release/release-notes-generator",
    "publish": "@semantic-release/npm",
    "success": false,
    "fail": false,
    "branches": [
      "main"
    ],
    "npmPublish": true,
    "tarballDir": "./*"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap-icons": "^1.9.1",
    "cheerio": "^1.0.0-rc.12",
    "core-js": "^3.24.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "ffmpeg": "0.0.4",
    "get-video-dimensions": "^1.0.0",
    "google-news-scraper": "^1.0.9",
    "json-2-csv": "^3.17.1",
    "lowdb": "^3.0.0",
    "node-stream-zip": "^1.15.0",
    "nodemailer": "^6.7.8",
    "pm2": "^5.2.0",
    "puppeteer": "^16.1.0",
    "puppeteer-extra": "^3.3.4",
    "request-promise-native": "^1.0.9",
    "sequelize": "^6.21.3",
    "sqlite-to-json": "^0.1.3",
    "sqlite3": "^5.0.11",
    "vue": "^2.7.8",
    "vue-router": "^3.5.4",
    "vuejs-datatable": "^2.0.0-alpha.7",
    "vuetify": "^2.6.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^4.5.19",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^28.1.3",
    "npm-check-updates": "^16.0.5",
    "prettier": "2.7.1",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "semantic-release": "^19.0.3",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-template-compiler": "^2.7.8",
    "vuetify-loader": "^1.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {},
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
