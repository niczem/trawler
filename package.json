{
  "name": "trawlr",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "all": "pm2 start ecosystem.config.js",
    "googlesearch:install": "cd GoogleSearchCrawler && pip install -r requirements.txt",
    "postinstall": "mkdir -p data&&touch data/db.json&&cp -n .env.template .env",
    "prettier:check": "prettier --check \"{src,services,datasources}/**/*.{vue,js,ts}\"",
    "prettier:write": "prettier --write \"{src,services,datasources}/**/*.{vue,js,ts}\"",
    "test": "jest",
    "semantic-release": "semantic-release",
    "generate-readme": "cd scripts && ./build-docu.sh"
  },
  "release": {
    "analyzeCommits": "@semantic-release/commit-analyzer",
    "verifyConditions": "@semantic-release/npm",
    "prepare": "@semantic-release/npm",
    "generateNotes": "@semantic-release/release-notes-generator",
    "publish": "@semantic-release/npm",
    "success": false,
    "fail": false,
    "branches": [
      "main"
    ],
    "npmPublish": true,
    "tarballDir": "./*"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "bootstrap-icons": "^1.5.0",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.17.2",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "ffmpeg": "0.0.4",
    "get-video-dimensions": "^1.0.0",
    "json-2-csv": "^3.14.4",
    "lowdb": "^1.0.0",
    "node-stream-zip": "^1.14.0",
    "nodemailer": "^6.6.3",
    "pm2": "^5.1.1",
    "puppeteer": "^5.5.0",
    "sequelize": "^5.22.4",
    "sqlite-to-json": "^0.1.3",
    "sqlite3": "^5.0.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuejs-datatable": "^2.0.0-alpha.7",
    "vuetify": "^2.5.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^26.6.3",
    "prettier": "2.1.1",
    "sass": "^1.39.0",
    "sass-loader": "^10.2.0",
    "semantic-release": "^19.0.3",
    "vue-cli-plugin-vuetify": "^2.1.1",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {},
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
